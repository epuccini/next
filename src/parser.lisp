          (setf expr-list (parse-arguments (cdr expr-list) *infinite-arguments*)))
        ((equal "prnl" (car expr-list))
         (let ((type (get-type expr-list)))
           (store-current-function "prnl")
           (if (search "array" type)
               (progn
